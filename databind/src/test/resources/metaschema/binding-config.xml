<?xml version="1.0" encoding="UTF-8"?>
<metaschema-bindings
	xmlns="https://csrc.nist.gov/ns/metaschema-binding/1.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="https://csrc.nist.gov/ns/metaschema-binding/1.0 ../../../main/xsd/metaschema-binding.xsd ">
	<model-binding
		namespace="http://csrc.nist.gov/ns/metaschema/testing/assembly">
		<java>
			<use-package-name>gov.nist.itl.metaschema.codegen.xml.example.assembly</use-package-name>
		</java>
	</model-binding>
	<metaschema-binding href="./metaschema.xml">
		<define-assembly-binding name="grandchild">
		<java><use-class-name>TheChild</use-class-name></java>
		</define-assembly-binding>
	</metaschema-binding>
</metaschema-bindings>