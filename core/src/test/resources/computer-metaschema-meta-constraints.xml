    xsi:schemaLocation="http://csrc.nist.gov/ns/oscal/metaschema/1.0 ../../../../core/metaschema/schema/xml/metaschema-meta-constraints.xsd">
    <import href="test"/>
    
    <context>
        <metapath target="/(computer|vendor)/@id"/>
        <constraints>
            <matches target="." datatype="uuid"/>
